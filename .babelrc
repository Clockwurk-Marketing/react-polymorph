{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": [
    "add-module-exports",
    "transform-decorators-legacy",
    "lodash",
    ["transform-runtime", {
      "polyfill": true,
      "regenerator": true
    }]
  ],
  "env": {
    "production": {
      "presets": ["react-optimize"],
      "plugins": ["babel-plugin-dev-expression"]
    },
    "development": {
      "presets": ["react-hmre"]
    }
  }
}
